{
    "name": "nuxt-mf-monorepo",
    "private": true,
    "packageManager": "pnpm@9.0.0",
    "scripts": {
        "dev": "pnpm turbo run dev --parallel",
        "dev:all": "pnpm turbo run dev --parallel",
        "dev:host": "pnpm --filter host dev",
        "dev:checkout": "pnpm --filter checkout dev",
        "dev:profile": "pnpm --filter profile dev",
        "dev:admin": "pnpm --filter admin dev",
        "build": "pnpm turbo run build",
        "build:all": "pnpm turbo run build",
        "build:host": "pnpm --filter host build",
        "build:checkout": "pnpm --filter checkout build",
        "build:profile": "pnpm --filter profile build",
        "build:admin": "pnpm --filter admin build",
        "start": "pnpm --filter host start",
        "preview:host": "pnpm --filter host start",
        "lint": "pnpm turbo run lint",
        "test:e2e": "pnpm turbo run test:e2e",
        "test:e2e:local": "PLAYWRIGHT_BASE_URL=http://localhost:3000 pnpm dlx playwright test",
        "clean": "pnpm turbo run clean",
        "dev:local": "bash ./start-local.sh",
        "start:local": "bash ./start-local.sh",
        "health:all": "node ./scripts/health-all.js",
        "sri:generate": "node ./scripts/generate-sri.js",
        "sri:verify": "node ./scripts/verify-remote-sri.js"
    },
    "devDependencies": {
        "@nuxt/webpack-builder": "^4.3.0",
        "@playwright/test": "^1.58.1",
        "css-loader": "^7.1.3",
        "esbuild-loader": "^4.4.2",
        "postcss-loader": "^8.2.0",
        "style-loader": "^4.0.0",
        "vue-loader": "17",
        "vue-style-loader": "^4.1.3",
        "webpack": "5",
        "webpack-cli": "^6.0.1",
        "webpack-hot-middleware": "^2.26.1"
    },
    "dependencies": {
        "defu": "^6.1.4",
        "ofetch": "^1.5.1",
        "ufo": "^1.6.3",
        "vue": "3",
        "vue-router": "4"
    }
}
