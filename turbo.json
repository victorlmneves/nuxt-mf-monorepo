{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "dev": {
            "cache": false
        },
        "build": {
            "dependsOn": ["^build"],
            "outputs": [".output/**", ".nuxt/**"]
        },
        "lint": {
            "outputs": []
        },
        "test:e2e": {
            "dependsOn": ["build"],
            "outputs": []
        },
        "clean": {
            "cache": false
        }
    }
}
